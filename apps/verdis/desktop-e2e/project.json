{
  "name": "verdis-desktop-e2e",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "apps/verdis/desktop-e2e/src",
  "targets": {
    "e2e": {
      "executor": "@nx/cypress:cypress",
      "options": {
        "cypressConfig": "apps/verdis/desktop-e2e/cypress.config.ts",
        "testingType": "e2e",
        "devServerTarget": "verdis-desktop:serve"
      },
      "configurations": {
        "production": {
          "devServerTarget": "verdis-desktop:serve:production"
        },
        "ci": {
          "devServerTarget": "verdis-desktop:serve-static"
        }
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"]
    }
  },
  "implicitDependencies": ["verdis-desktop"],
  "tags": []
}
